/*UBUNTU+NDK编译程序*/ 
cd testjni2
ndk-build
chmod 777 testjni2/libs/armeabi/px4_uploader

/*确认飞控设备已经连接到android主板*/
adb devices -l

/*通过adb传输px4_uploader到Android板*/
adb remount
adb push tesjni2/libs/armeabi/px4_uploader /system/bin

/*通过adb传输ardupilot镜像文件到Android板*/
adb remount
adb push /home/cuigaosheng/APM/arudpilot/ArduCopter/ArduCopter-v2.px4 /system/bin

/*通过adb进入android shell终端，执行程序*/
adb shell 
cd /system/bin
./px4_uploader --path ./system/bin/ArduCopter-v2.px4




